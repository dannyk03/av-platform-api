{
  "importOrderParserPlugins": [
    "typescript",
    "decorators-legacy"
  ],
  "importOrder": [
    "^@nestjs/(.*)$",
    "^@avo/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^(.*).module(.*)$",
    "^(.*).entity(.*)$",
    "^(.*).service(.*)$",
    "^(.*).serialization(.*)$",
    "^(.*).decorator(.*)$",
    "^(.*).dto(.*)$",
    "^(.*).interface(.*)$",
    "^@/(.*)$",
    "^[.*/]"
  ],
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderSeparation": true,
  "singleQuote": true,
  "trailingComma": "all"
}