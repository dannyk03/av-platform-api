{
  "name": "platform",
  "version": "0.1.8",
  "description": "Platform API",
  "repository": "https://github.com/jul-avo/platform-api",
  "author": "Avo team",
  "private": true,
  "license": "UNLICENSED",
  "namespace": "5e88e0cb-2833-458d-b78f-02137111888a",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "yarn migration:run && nest start",
    "start:prod": "node dist/src/main",
    "start:dev": "yarn migration:run && nest start --watch",
    "start:debug": "yarn migration:run && nest start --debug --watch",
    "format": "yarn prettier:check --write",
    "prettier:check": "prettier --list-different --ignore-path .gitignore . '*.{js,ts,md,yml,yaml,json}'",
    "eslint:check": "eslint --ignore-path .gitignore .",
    "lint:fix": "yarn eslint:check --fix && yarn format",
    "lint": "yarn eslint:check && yarn prettier:check",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "jest --config test/jest.json --detectOpenHandles --passWithNoTests --forceExit",
    "test:unit:watch": "yarn test:unit --watch",
    "test:integration": "jest --config integration/jest.json --detectOpenHandles --passWithNoTests --forceExit",
    "test:integration:watch": "yarn test:integration --watch",
    "test:cov": "yarn test:unit --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "is-ci || husky install",
    "deadcode": "ts-prune --project tsconfig.json",
    "spell": "yarn spell:src && yarn spell:test",
    "spell:src": "cspell lint --config cspell.json src/**/*.ts --color --gitignore --no-must-find-files --no-summary --no-progress || true",
    "spell:test": "cspell lint --config cspell.json test/**/*.ts --color --gitignore --no-must-find-files --no-summary --no-progress || true",
    "pre-commit": "lint-staged",
    "seed": "nestjs-command insert:system && nestjs-command insert:role-presets",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm migration:generate -d src/database/typeorm-cli/default.datasource.ts -p src/database/migrations/default/migration",
    "migration:create": "yarn typeorm  migration:create src/database/migrations/default/custom",
    "migration:run": "MIGRATION=true yarn typeorm migration:run -d src/database/typeorm-cli/default.datasource.ts",
    "migration:revert": "MIGRATION=true yarn typeorm migration:revert -d src/database/typeorm-cli/default.datasource.ts",
    "schema:drop:dev:danger": "yarn typeorm schema:drop -d src/database/typeorm-cli/default.datasource.ts",
    "db:recreate:dev:danger": "yarn schema:drop:dev:danger && yarn migration:run",
    "cache:clear": "yarn typeorm cache:clear -d src/database/typeorm-cli/default.datasource.ts",
    "prestart": "nestjs-command create:db"
  },
