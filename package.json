{
  "name": "platform",
  "version": "0.0.1",
  "description": "Platform API",
  "author": "Avo team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:prod": "node dist/main",
    "start:dev": "yarn prestart && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "format": "yarn prettier:check --write",
    "prettier:check": "prettier --list-different --ignore-path .gitignore . '*.{js,ts,md,yml,yaml,json}'",
    "eslint:check": "eslint --ignore-path .gitignore .",
    "lint:fix": "yarn eslint:check --fix && yarn format",
    "lint": "yarn eslint:check && yarn prettier:check",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "jest --config test/jest.json --detectOpenHandles --passWithNoTests --forceExit",
    "test:unit:watch": "yarn test:unit --watch",
    "test:integration": "jest --config integration/jest.json --detectOpenHandles --passWithNoTests --forceExit",
    "test:integration:watch": "yarn test:integration --watch",
    "test:cov": "yarn test:unit --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "is-ci || husky install",
    "deadcode": "ts-prune --project tsconfig.json",
    "pre-commit": "lint-staged",
    "seed": "nestjs-command insert:system && nestjs-command insert:role-presets",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm migration:generate -d src/database/typeorm-cli/default.datasource.ts -p src/database/migrations/default/migration",
    "migration:create": "yarn typeorm  migration:create src/database/migrations/default/custom",
    "migration:run": "yarn typeorm migration:run -d src/database/typeorm-cli/default.datasource.ts",
    "migration:revert": "yarn typeorm migration:revert -d src/database/typeorm-cli/default.datasource.ts",
    "schema:drop": "yarn typeorm schema:drop -d src/database/typeorm-cli/default.datasource.ts",
    "cache:clear": "yarn typeorm cache:clear -d src/database/typeorm-cli/default.datasource.ts",
    "prestart": "nestjs-command create:db"
  },
  "dependencies": {
    "@avo/casl": "^0.0.20",
    "@avo/type": "^0.0.18",
    "@aws-sdk/client-s3": "^3.142.0",
    "@faker-js/faker": "^7.3.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.8",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/terminus": "^9.1.0",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.30.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "cuid": "^2.1.8",
    "currency-codes": "^2.1.0",
    "currency-symbol-map": "^5.1.0",
    "customerio-node": "^3.3.3",
    "dayjs": "^1.11.4",
    "exceljs": "^4.3.0",
    "express-rate-limit": "^6.5.1",
    "geolib": "^3.3.3",
    "helmet": "^5.1.1",
    "iso-639-1": "^2.1.15",
    "libphonenumber-js": "^1.10.11",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.34",
    "morgan": "^1.10.0",
    "nanoid": "~3.3.4",
    "nest-winston": "^1.7.0",
    "nestjs-command": "^3.1.2",
    "nestjs-i18n": "^9.1.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rimraf": "^3.0.2",
    "rotating-file-stream": "^3.0.4",
    "rxjs": "^7.5.6",
    "slugify": "^1.6.5",
    "streamifier": "^0.1.1",
    "typeorm": "^0.3.7",
    "typeorm-extension": "^2.1.7",
    "ua-parser-js": "^1.0.2",
